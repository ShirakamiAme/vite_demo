{"ast":null,"code":"export default {\n  props: {\n    products: Array\n  },\n  methods: {\n    getStockLabel(stock) {\n      if (stock === 0) return '无货';\n      if (stock > 1000) return '库存充裕';\n      if (stock > 100) return '100+';\n      return stock.toString();\n    },\n    getStockClass(stock) {\n      if (stock === 0) return 'stock-gray';\n      if (stock > 1000) return 'stock-green';\n      if (stock > 100) return 'stock-yellow';\n      if (stock > 0) return 'stock-red';\n      return '';\n    }\n  }\n};","map":{"version":3,"names":["props","products","Array","methods","getStockLabel","stock","toString","getStockClass"],"sources":["D:\\计算机\\谭\\mobile_sales\\src\\components\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"products-grid\">\r\n    <div\r\n        v-for=\"product in products\"\r\n        :key=\"product.id\"\r\n        :class=\"['product-card', product.stock === 0 ? 'opacity-50' : '']\"\r\n    >\r\n      <div class=\"aspect-16-9\">\r\n        <img :src=\"product.image\" :alt=\"product.name\" />\r\n      </div>\r\n      <div class=\"product-info\">\r\n        <h3 :class=\"['product-name', product.stock === 0 ? 'line-through' : '']\">{{ product.name }}</h3>\r\n        <p class=\"product-price\">¥{{ product.price.toFixed(2) }}</p>\r\n        <div>\r\n          <span\r\n              class=\"stock-label\"\r\n              :class=\"getStockClass(product.stock)\"\r\n          >\r\n            {{ getStockLabel(product.stock) }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    products: Array\r\n  },\r\n  methods: {\r\n    getStockLabel(stock) {\r\n      if (stock === 0) return '无货';\r\n      if (stock > 1000) return '库存充裕';\r\n      if (stock > 100) return '100+';\r\n      return stock.toString();\r\n    },\r\n    getStockClass(stock) {\r\n      if (stock === 0) return 'stock-gray';\r\n      if (stock > 1000) return 'stock-green';\r\n      if (stock > 100) return 'stock-yellow';\r\n      if (stock > 0) return 'stock-red';\r\n      return '';\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA2BA,eAAe;EACbA,KAAK,EAAE;IACLC,QAAQ,EAAEC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,KAAK,EAAE;MACnB,IAAIA,KAAI,KAAM,CAAC,EAAE,OAAO,IAAI;MAC5B,IAAIA,KAAI,GAAI,IAAI,EAAE,OAAO,MAAM;MAC/B,IAAIA,KAAI,GAAI,GAAG,EAAE,OAAO,MAAM;MAC9B,OAAOA,KAAK,CAACC,QAAQ,CAAC,CAAC;IACzB,CAAC;IACDC,aAAaA,CAACF,KAAK,EAAE;MACnB,IAAIA,KAAI,KAAM,CAAC,EAAE,OAAO,YAAY;MACpC,IAAIA,KAAI,GAAI,IAAI,EAAE,OAAO,aAAa;MACtC,IAAIA,KAAI,GAAI,GAAG,EAAE,OAAO,cAAc;MACtC,IAAIA,KAAI,GAAI,CAAC,EAAE,OAAO,WAAW;MACjC,OAAO,EAAE;IACX;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}