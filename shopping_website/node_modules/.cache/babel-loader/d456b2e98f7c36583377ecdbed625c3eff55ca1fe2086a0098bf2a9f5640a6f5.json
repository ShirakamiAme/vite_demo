on\" @click.prevent=\"goToCart\">\r\n          <font-awesome-icon :icon=\"['fas', 'shopping-cart']\" />\r\n          <span class=\"cart-count\" v-if=\"cartCount > 0\">{{ cartCount }}</span>\r\n        </a>\r\n      </div>\r\n\r\n      <!-- 用户下拉菜单 -->\r\n      <div v-if=\"isLoggedIn && showUserMenu\" class=\"user-dropdown\">\r\n        <div class=\"user-info\">\r\n          <div class=\"user-name\">{{ username }}</div>\r\n          <div class=\"user-email\">{{ userEmail }}</div>\r\n        </div>\r\n        <ul class=\"user-menu\">\r\n          <li><a href=\"/profile\">个人资料</a></li>\r\n          <li><a href=\"/orders\">我的订单</a></li>\r\n          <li><a href=\"/settings\">设置</a></li>\r\n          <li><button @click=\"logout\">退出登录</button></li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport useUserStore from '@/composables/useUserStore';\r\n\r\nconst router = useRouter();\r\n\r\n// 使用外部定义的状态管理\r\nconst { isLoggedIn, username, userEmail, cartCount } = useUserStore();\r\nconst showUserMenu = ref(false);\r\nconst searchQuery = ref('');\r\n\r\n// 初始化加载用户状态\r\nonMounted(() => {\r\n  loadUserState();\r\n});\r\n\r\n// 加载用户状态\r\nfunction loadUserState() {\r\n  const savedState = localStorage.getItem('userState');\r\n  if (savedState) {\r\n    const state = JSON.parse(savedState);\r\n    isLoggedIn.value = state.isLoggedIn || false;\r\n    username.value = state.username || '';\r\n    userEmail.value = state.userEmail || '';\r\n    cartCount.value = state.cartCount || 0;\r\n  }\r\n}\r\n\r\n// 搜索功能\r\nfunction performSearch() {\r\n  if (searchQuery.value.trim()) {\r\n    router.push({ path: '/search', query: { q: searchQuery.value } });\r\n  }\r\n}\r\n\r\n// 登录/登出处理\r\nfunction handleLoginClick() {\r\n  if (isLoggedIn.value) {\r\n    showUserMenu.value = !showUserMenu.value;\r\n  } else {\r\n    router.push('/login');\r\n  }\r\n}\r\n\r\n// 用户下拉菜单点击外部区域关闭\r\ndocument.addEventListener('click', (event) => {\r\n  const dropdown = document.querySelector('.user-dropdown');\r\n  const navIcon = document.querySelector('.nav-icon');\r\n\r\n  if (showUserMenu.value && dropdown && !dropdown.contains(event.target) && !navIcon.contains(event.target)) {\r\n    showUserMenu.value = false;\r\n  }\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.logo font-awesome-icon {\r\n  font-size: 24px;\r\n}\r\n\r\n.nav-icons font-awesome-icon {\r\n  font-size: 20px;\r\n}\r\n\r\nheader {\r\n  background-color: var(--color4);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 100;\r\n  box-shadow: var(--shadow);\r\n}\r\n\r\n.header-content {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.logo {\r\n  font-family: 'Playfair Display', serif;\r\n  font-size: 28px;\r\n  font-weight: 700;\r\n  color: var(--color2);\r\n  text-decoration: none;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.logo i {\r\n  font-size: 24px;\r\n}\r\n\r\n.search {\r\n  flex: 0 0 50%;\r\n  max-width: 500px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: relative;\r\n}\r\n\r\n.search__input {\r\n  font-family: \"Noto Sans SC\", sans-serif;\r\n  font-size: 17px;\r\n  color: #333333;\r\n  background-color: #f4f2f2;\r\n  border: none;\r\n  padding: 14px 72px 14px 42px;\r\n  border-radius: 1000px;\r\n  width: 94%;\r\n  margin-left: -34px;\r\n  margin-right: -66px;\r\n  transition: all 0.2s;\r\n  outline: none;\r\n}\r\n\r\n#search__input::placeholder {\r\n  font-size: 18px;\r\n}\r\n\r\n.search__input:focus {\r\n  width: 100%;\r\n  background-color: #f0eeee;\r\n}\r\n\r\n.search__input::-webkit-input-placeholder {\r\n  font-size: 12px;\r\n  text-transform: uppercase;\r\n  letter-spacing: 5px;\r\n  font-weight: 500;\r\n  color: #999999;\r\n}\r\n\r\n.search__button,\r\n.mic__button,\r\n.picture__button {\r\n  border: none;\r\n  