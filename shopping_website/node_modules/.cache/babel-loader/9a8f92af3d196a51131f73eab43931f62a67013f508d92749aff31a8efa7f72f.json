8-0.12,0.77-0.32c0.2-0.2,0.32-0.47,0.32-0.77 V84.21H80.74c-5.45,3.86-12.11,6.13-19.3,6.13C54.25,90.34,47.59,88.07,42.14,84.21L42.14,84.21z M43.26,81.81 c0.04,0.03,0.08,0.05,0.12,0.08c5.08,3.68,11.32,5.84,18.06,5.84s12.99-2.17,18.06-5.84c0.04-0.03,0.08-0.06,0.12-0.08 c1.29-0.94,2.5-1.98,3.62-3.1c5.58-5.58,9.03-13.29,9.03-21.8c0-8.51-3.45-16.22-9.03-21.8c-0.73-0.73-1.5-1.43-2.31-2.09 c-0.03-0.02-0.06-0.05-0.09-0.07c-5.3-4.3-12.05-6.87-19.4-6.87c-7.35,0-14.1,2.57-19.4,6.87c-0.03,0.03-0.06,0.05-0.09,0.07 c-0.8,0.66-1.58,1.35-2.31,2.09c-5.58,5.58-9.03,13.29-9.03,21.8c0,8.51,3.45,16.22,9.03,21.8C40.76,79.83,41.97,80.87,43.26,81.81 L43.26,81.81z M83.98,81.61h36.3V33.27H85.09c6.05,6.05,9.79,14.41,9.79,23.63c0,9.23-3.74,17.59-9.8,23.64 C84.72,80.91,84.35,81.26,83.98,81.61L83.98,81.61z M2.6,30.67h38.11c5.7-4.51,12.9-7.2,20.73-7.2s15.03,2.69,20.73,7.2h38.11 V16.24c0-0.3-0.12-0.58-0.32-0.77c-0.2-0.2-0.47-0.32-0.77-0.32H3.7c-0.3,0-0.58,0.12-0.78,0.32c-0.2,0.2-0.32,0.47-0.32,0.77 V30.67L2.6,30.67z\"></path>\r\n              </g>\r\n            </svg>\r\n          </div>\r\n        </button>\r\n      </form>\r\n\r\n      <div class=\"nav-icons\">\r\n        <a\r\n            :href=\"isLoggedIn ? '#' : '/login'\"\r\n            class=\"nav-icon\"\r\n            :class=\"isLoggedIn ? 'logged_in' : 'not_logged_in'\"\r\n            @click.prevent=\"handleLoginClick\"\r\n        >\r\n          <font-awesome-icon :icon=\"['fas', 'user']\" />\r\n          <span v-if=\"isLoggedIn\" class=\"username\">{{ username }}</span>\r\n        </a>\r\n\r\n        <a href=\"#\" class=\"nav-icon\" @click.prevent=\"toggleWishlist\">\r\n          <font-awesome-icon :icon=\"['fas', 'heart']\" />\r\n        </a>\r\n\r\n        <a href=\"/shoppingcart\" class=\"nav-icon cart-icon\" @click.prevent=\"goToCart\">\r\n          <font-awesome-icon :icon=\"['fas', 'shopping-cart']\" />\r\n          <span class=\"cart-count\" v-if=\"cartCount > 0\">{{ cartCount }}</span>\r\n        </a>\r\n      </div>\r\n\r\n      <!-- 用户下拉菜单 -->\r\n      <div v-if=\"isLoggedIn && showUserMenu\" class=\"user-dropdown\">\r\n        <div class=\"user-info\">\r\n          <div class=\"user-name\">{{ username }}</div>\r\n          <div class=\"user-email\">{{ userEmail }}</div>\r\n        </div>\r\n        <ul class=\"user-menu\">\r\n          <li><a href=\"/profile\">个人资料</a></li>\r\n          <li><a href=\"/orders\">我的订单</a></li>\r\n          <li><a href=\"/settings\">设置</a></li>\r\n          <li><button @click=\"logout\">退出登录</button></li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport useUserStore from '@/composables/useUserStore'; // 根据你的路径修改\r\n\r\n/* global webkitSpeechRecognition */\r\n\r\nconst router = useRouter();\r\n\r\n// 使用外部定义的状态管理\r\nconst { isLoggedIn, username, userEmail, cartCount } = useUserStore();\r\nconst showUserMenu = ref(false);\r\n\r\n// 初始化加载用户状态\r\nonMounted(() => {\r\n  loadUserState();\r\n});\r\n\r\n// 加载用户状态\r\nfunction loadUserState() {\r\n  const savedState = localStorage.getItem('userState');\r\n  if (savedState) {\r\n    const state = JSON.parse(savedState);\r\n    isLoggedIn.value = state.isLoggedIn || false;\r\n    username.value = state.username || '';\r\n    userEmail.value = state.userEmail || '';\r\n    cartCount.value = state.cartCount || 0;\r\n  }\r\n}\r\n\r\n// 搜索功能\r\nfunction performSearch() {\r\n  if (searchQuery.value.trim()) {\r\n    router.push({ path: '/search', query: { q: searchQuery.value } });\r\n  }\r\n}\r\n\r\n// 语音搜索\r\nfunction startVoiceSearch() {\r\n  if ('webkitSpeechRecognition' in window) {\r\n    const recognition = new window.webkitSpeechRecognition();\r\n    recognition.lang = 'zh-CN';\r\n    recognition.onresult = (event) => {\r\n      searchQuery.value = event.results[0][0].transcript;\r\n      performSearch();\r\n    };\r\n    recognition.start();\r\n  } else {\r\n    alert('您的浏览器不支持语音识别功能');\r\n  }\r\n}\r\n\r\n// 图像搜索（占位函数）\r\nfunction openImageSearch() {\r\n  alert('图像搜索功能暂未实现');\r\n}\r\n\r\n// 登录/登出处理\r\nfunction handleLoginClick() {\r\n  if (isLoggedIn.value) {\r\n    showUserMenu.value = !showUserMenu.value;\r\n  } else {\r\n    router.push('/login');\r\n  }\r\n}\r\n\r\n// 用户下拉菜单点击外部区域关闭\r\ndocument.addEventListener('click', (event) => {\r\n  const dropdown = document.querySelector('.user-dropdown');\r\n  const navIcon = document.querySelector('.nav-icon');\r\n\r\n  if (showUserMenu.value && dropdown && !dropdown.contains(event.target) && !navIcon.contains(event.target)) {\r\n    showUserMenu.value = false;\r\n  }\r\n});\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.logo font-awesome-icon {\r\n  font-size: 24px;\r\n}\r\n\r\n.nav-icons font-awesome-icon {\r\n  font-size: 20px;\r\n}\r\n\r\nheader {\r\n  background-color: var(--color4);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 100;\r\n  box-shadow: var(--shadow);\r\n}\r\n\r\n.header-content {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.logo {\r\n  font-family: 'Playfair Display', serif;\r\n  font-size: 28px;\r\n  font-weight: 700;\r\n  color: var(--color2);\r\n  text-decoration: none;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.logo i {\r\n  font-size: 24px;\r\n}\r\n\r\n.search {\r\n  flex: 0 0 50%;\r\n  max-width: 500px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: relative;\r\n}\r\n\r\n.search__input {\r\n  font-family: \"Noto Sans SC\", sans-serif;\r\n  font-size: 17px;\r\n  color: #333333;\r\n  background-color: #f4f2f2;\r\n  border: none;\r\n  padding: 14px 72px 14px 42px;\r\n  border-radius: 1000px;\r\n  width: 94%;\r\n  margin-left: -34px;\r\n  margin-right: -66px;\r\n  transition: all 0.2s;\r\n  outline: none;\r\n}\r\n\r\n#search__input::placeholder {\r\n  font-size: 18px;\r\n}\r\n\r\n.search__input:focus {\r\n  width: 100%;\r\n